{
  "functions": {
    "src/sendmail.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/src/index.html" },
    { "source": "/about", "destination": "/src/about.html" },
    { "source": "/projects", "destination": "/src/projects.html" },
    { "source": "/blog", "destination": "/src/blog.html" },
    { "source": "/blog-details", "destination": "/src/blog-details.html" },
    { "source": "/service", "destination": "/src/service.html" },
    { "source": "/contact", "destination": "/src/contact.html" },
    { "source": "/single-project", "destination": "/src/single-project.html" },
    { "source": "/assets/(.*)", "destination": "/src/assets/$1" },
    { "source": "/components/(.*)", "destination": "/src/components/$1" },
    { "source": "/utils/(.*)", "destination": "/src/utils/$1" }
  ],
  "headers": [
    {
      "source": "/src/assets/js/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/src/assets/css/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "/src/(.*).html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}