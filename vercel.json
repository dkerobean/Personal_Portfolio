{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/projects", "destination": "/projects.html" },
    { "source": "/blog", "destination": "/blog.html" },
    { "source": "/blog-details", "destination": "/blog-details.html" },
    { "source": "/service", "destination": "/service.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/single-project", "destination": "/single-project.html" },
    { "source": "/test", "destination": "/test.html" },
    { "source": "/test-assets", "destination": "/test-assets.html" }
  ],
  "headers": [
    {
      "source": "/dist/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/js/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/css/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}